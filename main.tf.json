{
    "module": {
        "do": {
            "source": "./modules/do",
            "do_region": "${var.do_region}",
            "do_token": "${var.do_token}",
            "do_k8s_version": "${var.do_k8s_version}"
        },
        "azure": {
            "source": "./modules/azure",
            "az_sp_id": "${var.az_sp_id}",
            "az_sp_secret": "${var.az_sp_secret}",
            "az_location": "${var.az_region}",
            "az_rg": "${var.az_rg}",
            "az_environment": "${var.az_environment}",
            "az_prefix": "${var.az_prefix}",
            "az_vm_size": "${var.az_vm_size}",
            "az_k8s_version": "${var.az_k8s_version}"
        }
    }
}
